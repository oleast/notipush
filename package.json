    "@types/node-fetch": "^2.1.4",
    "@types/node-schedule": "^1.2.2",
    "@types/web-push": "^3.3.0",
    "nodaemon": "^0.0.5",
    "prettier": "^1.16.3",
    "prettier-check": "^2.0.0",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.3.1"
  },
  "dependencies": {
    "@koa/cors": "^2.2.3",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "node-fetch": "^2.3.0",
